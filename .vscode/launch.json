{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Run All",
      "configurations": ["Run Backend", "Run Blazor"]
    }
  ],
  "configurations": [
    {
      "name": "Run Backend",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": ["run", "--project", "${workspaceFolder}/Backend", "--urls", "https://localhost:7276"],
      "cwd": "${workspaceFolder}/Backend",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Run Blazor",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": ["run", "--project", "${workspaceFolder}/ReportWithRealTime","--urls","https://localhost:7166"],
      "cwd": "${workspaceFolder}/ReportWithRealTime",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
